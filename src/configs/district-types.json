{
  "$schema": "./schemas/district-types.json",
  "residential": {
    "activityRequirementModifier": 1,
    "primaryActivityimeMultipliers": {
      "completionTime": 1,
      "generationTime": 5
    },
    "parameters": {
      "experience": { "multiplier": 1, "base": 1.5 },
      "money": { "multiplier": 1, "base": 1.5 },
      "developmentPoints": { "multiplier": 1, "base": 1.5 },
      "influence": {
        "requirements": { "multiplier": 1000, "base": 10 },
        "pointsMultiplier": { "multiplier": 1, "base": 1 }
      },
      "synchronization": { "multiplier": 10, "base": 1.1 },
      "connectivity": {
        "pointsMultiplier": { "multiplier": 1, "base": 1.5 },
        "programPointsMultiplier": { "multiplier": 1, "base": 1.1 }
      },
      "codeBase": {
        "pointsMultiplier": { "multiplier": 1, "base": 1.5 },
        "pointsToSoftCap": 86400,
        "logBase": 2
      },
      "computationalBase": {
        "pointsMultiplier": { "multiplier": 1, "base": 1.5 },
        "pointsToSoftCap": 86400,
        "logBase": 2
      },
      "rewards": {
        "pointsMultiplier": { "multiplier": 1, "base": 1.5 },
        "pointsToSoftCap": 86400,
        "logBase": 2
      },
      "processCompletionSpeed": { "multiplier": 1, "base": 1.1 },
      "experienceShareMultiplier": { "multiplier": 1, "base": 1.1 }
    }
  },
  "corpoDistrict": {
    "activityRequirementModifier": 1,
    "primaryActivityimeMultipliers": {
      "completionTime": 1,
      "generationTime": 5
    },
    "parameters": {
      "experience": { "multiplier": 1, "base": 1.1 },
      "money": { "multiplier": 10, "base": 1.1 },
      "developmentPoints": { "multiplier": 5, "base": 1.1 },
      "influence": {
        "requirements": { "multiplier": 60000, "base": 1.5 },
        "pointsMultiplier": { "multiplier": 1, "base": 1 }
      },
      "synchronization": {
        "multiplier": 30,
        "base": 1.1
      },
      "connectivity": {
        "pointsMultiplier": { "multiplier": 5, "base": 1.5 },
        "programPointsMultiplier": { "multiplier": 5, "base": 1.5 }
      },
      "codeBase": {
        "pointsMultiplier": { "multiplier": 5, "base": 1.5 },
        "pointsToSoftCap": 86400,
        "logBase": 20
      },
      "computationalBase": {
        "pointsMultiplier": { "multiplier": 5, "base": 1.5 },
        "pointsToSoftCap": 86400,
        "logBase": 20
      },
      "rewards": {
        "pointsMultiplier": { "multiplier": 3, "base": 1.5 },
        "pointsToSoftCap": 86400,
        "logBase": 20
      },
      "processCompletionSpeed": { "multiplier": 1, "base": 1.1 },
      "experienceShareMultiplier": { "multiplier": 1, "base": 1.1 }
    }
  }
}
